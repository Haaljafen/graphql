{
  "rewrites": [
    {
      "source": "/profile",
      "destination": "/index.html"
    },
    {
      "source": "/login", 
      "destination": "/index.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404.html"
    }
  ]
}
